#
# @TEST-EXEC:  hilti-build %INPUT -o a.out
# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  btest-diff output

module Main

import Hilti

void run() {
    local int<32> v
    local ref<map<string, int<32>>> m

    m = new map<string, int<32>>

    map.insert m "Foo" 10
    map.insert m "Bar" 20

    v = map.get m "FooBar"
    call Hilti::print(v)
}
