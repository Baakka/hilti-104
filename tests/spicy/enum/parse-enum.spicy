    a: uint16;
    b: uint16 &convert=Foo($$);
    c: uint16 &convert=Foo($$);
    d: uint16 &convert=Bar($$);

    on %done {
        print self.a;
        print self.b;
        print self.c;
        print self.d;
        }
};
