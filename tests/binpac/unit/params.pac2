module Mini;

export type test = unit {
    a: bytes &length=4;
    foo: Foo(self, 42, self.a, 2);
    b: bytes &length=6;

    on %done { print self; }
};

type Foo = unit(t: test, n: int8, b: bytes, c: uint8) {
    i: int8 {
        print t, n, b;
    }

    b: bytes &length=c;
};
