module Mini;

export type test = unit {
    a: bytes &length=4;
    b: bytes &length=6 &default=b"foo";
    c: int<32> &default=42;

    on a { print self, self.a, self.b, self.c; }
    on %done { print self, self.a, self.b, self.c; }
};
