module PrintCerts;

import BinPAC;

import parsers/ms-cert;

on MSCerts::SerializedCertificateEntry::certificate
{
    print "-----BEGIN CERTIFICATE-----";
    print BinPAC::base64_encode(self.certificate);
    print "-----END CERTIFICATE-----\n";
}
