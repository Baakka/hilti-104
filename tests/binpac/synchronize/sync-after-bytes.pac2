module Mini;

export type test1 = unit {
    l: list<test2> &while($$.x != b"90") &synchronize;

    on %done   { print "%done:", self; }
    on %sync   { print "%synced"; }
};

type test2 = unit {
       %synchronize-after = b"ABC";

       : b"abc";
       x: bytes &length=2;

};
