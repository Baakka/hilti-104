module HTTP;

const LINE = /[^\n]+\n/;
const DASHES = /---\n/;

type Item = unit(t: Test) {
    line:   LINE
        { print t.lines; }
};

export type Test = unit {
    lines: list<Item(self)> &until($$.line == b"---\n");
    last: LINE;
    };


