module Main

import Hilti

void foo(int<32> n) {
    call Hilti::print ("Yippie!")
    call Hilti::print (n)
}

void run() {

    local ref<timer> t
    t = new timer foo (42)
    timer_mgr.schedule time(10.0) t
    timer_mgr.advance_global time(20.0)

    return.void
}


