module Main

import Hilti

type Tag = { A, B, C, D, E }
global ref<set<Tag>> disabled_analyzers = set<Tag>(A, B, C)

void run() {

    local ref<list<int<32>>> l
    local string s
    local string addl

    l = list<int<32>>(1,2,3,4,5)

    call Hilti::print ("A", True)

    for ( x in l ) {
        for ( y in l ) {
            s = call Hilti::fmt ("%d %d", (x, y))
            call Hilti::print (s)
        }
    }

    call Hilti::print ("B", True)

    return.void
}

