module Main

import Hilti

void run() {
    local ref<bytes> b
    local string f

    b = bytes.strip b" ABC "
    f = call Hilti::fmt ("|%s|", (b))
    call Hilti::print (f)

    b = bytes.strip b"ABC"
    f = call Hilti::fmt ("|%s|", (b))
    call Hilti::print (f)

    b = bytes.strip b"    ABC"
    f = call Hilti::fmt ("|%s|", (b))
    call Hilti::print (f)

    b = bytes.strip b"ABC    "
    f = call Hilti::fmt ("|%s|", (b))
    call Hilti::print (f)

    b = bytes.strip b""
    f = call Hilti::fmt ("|%s|", (b))
    call Hilti::print (f)

    b = bytes.strip b"     "
    f = call Hilti::fmt ("|%s|", (b))
    call Hilti::print (f)

    b = bytes.strip b" "
    f = call Hilti::fmt ("|%s|", (b))
    call Hilti::print (f)
}
