#
# @TEST-EXEC:  hilti-build %INPUT -o a.out
# @TEST-EXEC:  ./a.out >output 2>&1
# @TEST-EXEC:  btest-diff output

module Main

import Hilti

void run() {
    local int<32> s
    local bool b
    local ref<timer_mgr> t
    local ref<list<string>> m

    local iterator<list<string>> i
    local string str

    t = new timer_mgr
    m = new list<string> t
    list.timeout m Hilti::ExpireStrategy::Access interval(20.0)

    list.push_back m "A-0"
    list.push_back m "B-0"

    timer_mgr.advance t time(5.0)
    list.push_back m "C-5"
    list.push_back m "D-5"

    timer_mgr.advance t time(10.0)
    list.push_back m "E-10"
    list.push_back m "F-10"

    i = begin m
    str = deref i

    call Hilti::print(m)
    call Hilti::print("")

    timer_mgr.advance t time(10.0)
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

    timer_mgr.advance t time(20.0)
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

    timer_mgr.advance t time(25.0)
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

    timer_mgr.advance t time(50.0)
    call Hilti::print(m)
    call Hilti::print(t)
    call Hilti::print("")

}
