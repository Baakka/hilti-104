    debug.pop_indent
    debug.msg "bro" "- %s -> %s" ("direct call", (b"direct call"))
    debug.pop_indent
    __tmp_result = call foo1 (b"direct call")
    debug.msg "bro" "- %s -> %s" ("foo1(direct call)", __tmp_result)
--
    debug.pop_indent
    __tmp_val = call LibBro::h2b_bytes (b"indirect call")
    __tmp_rval = call LibBro::call_legacy_result (f, (__tmp_val))
