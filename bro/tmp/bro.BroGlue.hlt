module BroGlue

import Hilti
import LibBro

export to_lower

ref<bytes> to_lower(ref<bytes> str)
{

  local LibBro::BroVal __tmp_val
  local LibBro::BroVal __tmp_rval
  local ref<bytes> __tmp_b2h
  
  
    __tmp_val = call LibBro::h2b_bytes (str)
    __tmp_rval = call LibBro::call_bif_result (b"to_lower", (__tmp_val))
    __tmp_b2h = call LibBro::b2h_string (__tmp_rval)
    return.result __tmp_b2h
  
}



block.end

