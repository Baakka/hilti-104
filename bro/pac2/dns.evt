grammar dns.pac2;

protocol analyzer pac2::SSH over UDP:
    parse with DNS::Message,
    port 53/udp,
    replaces DNS;

on DNS::Question -> event my_dns_request($conn, self.qname.name, self.qtype, self.qclass);
